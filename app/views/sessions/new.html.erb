<% provide(:title, "Log in") %>
<h1>Log in</h1>
<div>
  <%= form_for(:session, url: login_path) do |f| %>

  <%= f.label :login_id %>
  <%= f.text_field :login_id%>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>
  <% end %>

  <p>New user? <%= link_to "Sign up now!", signup_path %></p>
</div>
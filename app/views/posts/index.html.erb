<div class="container">
  <div class="row">
    <div class="col-12">
      <div class="col-8 mx-auto">
        <h1>Beerckathon</h1>
        <% @posts.each do |post|%>
          <div>
            <%= post.content%>
          </div>
          <div>
            <%= post.created_at%>
            <%= post.updated_at%>
          </div>
        <%end%>
      </div>
    </div>
  </div>
</div>
